@article{THAA4111,
	author = {Carlos Borches},
	title = {Julio Rey Pastor y el desarrollo de la matemática como disciplina  científica en la Argentina},
	journal = {Temas de Historia Argentina y Americana},
	volume = {1},
	number = {30},
	year = {2022},
	keywords = {Rey Pastor; Matemáticas; Facultad de Ciencias Exactas, Físicas y Naturales; Universidad de Buenos Aires; Intelectuales},
	abstract = {El intelectual Julio Rey Pastor visitó la Argentina de principios del siglo XX.Durante el mismo se vinculó con la Facultad de Ciencias Exactas, Físicas y Naturales de la Universidad de Buenos Aires, desde donde construyó vínculos sociales. Analizar su estancia y recepción, permitirá ahondar en las vinculaciones previas de las ciencias matemáticas rioplatenses con España desde la época tardo-virreinal hasta los antecedentes inmediatos a la llegada de Rey Pastor a la cátedra Marcelino Ménendez y Pelayo de la UBA. Desde allí se podrá estudiar las características de este matemático y las prácticas y discursos científicos y académicos de los que formó parte en Buenos Aires.},
	issn = {2618-1924},	pages = {45--60},
	url = {https://erevistas.uca.edu.ar/index.php/THAA/article/view/4111}
}

